{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "root": "projects/tennis-breakdown",
        "submodules": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "projects/tennis-breakdown/$1"
    }
  ]
}
